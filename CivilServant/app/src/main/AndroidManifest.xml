<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a140.civilservant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.ExamActivity"
            android:label="@string/activity_exam"/>
        <activity android:name=".ui.YanyuActivity"
            android:label="@string/activity_yanyu"/>
        <activity android:name=".ui.ShulActivity"
            android:label="@string/activity_shul"/>
        <activity android:name=".ui.HistoryActivity"
            android:label="@string/activity_history"/>
        <activity android:name=".ui.BasicActivity"
            android:label="@string/activity_basic"/>
        <activity android:name=".ui.ProcessActivity"
            android:label="@string/activity_process"/>
        <activity android:name=".ui.AnnouncementActivity"
            android:label="@string/activity_announcement"/>
        <activity android:name=".ui.TimeActivity"
            android:label="@string/activity_time"/>
        <activity android:name=".ui.InstitutionActivity"
            android:label="@string/activity_institution"/>
        <activity android:name=".ui.SecretActivity"
            android:label="@string/activity_secret"/>
        <activity android:name=".ui.TitleActivity"
            android:label="@string/activity_title"/>
    </application>

</manifest>